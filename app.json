{
  "env": {
    "DATABASE_URL": {
      "description": "URL to connect to PostgreSQL",
      "development_value": "postgres://localhost:5432/canvas_pro_api_dev"
    },
    "NODE_ENV": {
      "description": "Environment to run Node in",
      "development_value": "development",
      "value": "production"
    },
    "PORT": {
      "description": "Port on which to run the app",
      "development_value": "4001",
      "required": false
    },
    "REDIS_URL": {
      "description": "URL to connect to Redis",
      "development_value": "redis://localhost:6379"
    }
  }
}
